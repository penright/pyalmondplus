#!/home/paul/pyalmondplus/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyalmondplus','console_scripts','pyalmondplus'
__requires__ = 'pyalmondplus'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyalmondplus', 'console_scripts', 'pyalmondplus')()
    )
